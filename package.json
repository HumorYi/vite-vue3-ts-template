{
  "name": "vite-vue3-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "lint:stylelint": "stylelint src/**/*.{css,less,scss,sass,vue,html} --fix",
    "lint:prettier": "prettier --write --log-level log \"src/**/*.{js,ts,json,tsx,css,less,scss,sass,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "lint-staged": {
    "*.{js,cjs,ts,json,tsx,css,less,scss,sass,vue,html,md}": [
      "eslint src --ext .js,.jsx,.ts,.tsx,.vue --fix",
      "stylelint src/**/*.{css,less,scss,sass,vue,html} --fix",
      "prettier --write --log-level log \"src/**/*.{js,ts,json,tsx,css,less,scss,sass,vue,html,md}\"",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vueuse/core": "^13.1.0",
    "axios": "^1.9.0",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.2",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.14.0",
    "reset.css": "^2.0.2",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.2",
    "@types/qs": "^6.9.18",
    "@types/vue": "^1.0.31",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@unocss/preset-rem-to-px": "^66.1.0",
    "@vant/auto-import-resolver": "^1.3.0",
    "@vitejs/plugin-legacy": "^6.1.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.1",
    "eslint": "^9.25.1",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "lightningcss": "^1.29.3",
    "lint-staged": "^15.5.1",
    "path": "^0.12.7",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.87.0",
    "stylelint": "^16.19.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "terser": "^5.39.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "unocss": "^66.1.2",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.2.0",
    "vite-bundle-analyzer": "^0.19.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-restart": "^0.4.2",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vue-component-type-helpers": "^2.2.8",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.4"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
